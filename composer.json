{
    "name": "soheilrt/laravel-adobe-connect-client",
    "description": "Adobe Connect Client Package For Laravel Based Applications",
    "keywords": [
        "adobe",
        "adobe connect",
        "laravel",
        "seminar",
        "virtual classroom",
        "webinar"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Soheil Rahmat",
            "email": "soheilrt314@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "laravel/framework": "~5.8 || ^6 || ^7",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "ext-fileinfo": "*"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "~3.8||~4.0||~5.0"
    },
    "autoload": {
        "psr-4": {
            "Soheilrt\\AdobeConnectClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Soheilrt\\AdobeConnectClient\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Soheilrt\\AdobeConnectClient\\AdobeConnectServiceProvider"
            ]
        }
    }
}
